package demo;

public class DemoTest {
int a=10;
int b=20;
	public int add(int a, int b) {
		return a+b;
	}
	public void add1(int a, int b) {
		int c=a+b;
		System.out.println(c);
	}
	
	public static void main(String[] args) {
	DemoTest d=new DemoTest();
	DemoTest d1=new DemoTest();
	d.add(10, 60);
	d1.add1(30, 70);
	System.out.println(d);

	}

}
